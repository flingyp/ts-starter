{
  "name": "pag-name",
  "description": "Starter template for TypeScript library",
  "type": "module",
  "version": "0.0.0",
  "author": "flingyp",
  "license": "MIT",
  "scripts": {
    "dev": "tsup --watch --sourcemap",
    "build": "tsup --minify"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "keywords": [
    "typescript",
    "library",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/flingyp"
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  }
}
